
/* 遮罩 */
.mask{
  width:100%;
  height:100%;
  position:absolute;
  background-color:#999;
  z-index:2;
  top:0;
  left:0;
  opacity:0;
}

/* 选项卡样式 */
 .actives{
  background-color: #e74c3c;
}

.unactives{
  color: white;
  background-color:#2c3e50;
}
.itemList{
  padding:10rpx 20rpx;
  border-radius: 35rpx;
  font-size: 20rpx;
  color: white;
  box-sizing: border-box;
}
